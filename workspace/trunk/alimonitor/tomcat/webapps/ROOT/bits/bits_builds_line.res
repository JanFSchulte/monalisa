<tr bgcolor="<<:bgcolor:>>" class="table_row">
    <td align=left class="table_row"><<:date:>></td>
    <td align=right class="table_row"><<:build_time:>></td>
    <td align=center class="table_row"><a class="link" href="javascript:void(0)" onClick="return setSelectOption(document.forms['form_build'], 'filter_state', '<<:state js:>>');"><font color="<<:build_result:>>"><b><<:state:>></b></font></td>
    <td align=right class="table_row"><a class="link" href="javascript:void(0)" onClick="return setSelectOption(document.forms['form_build'], 'filter_os', '<<:os js:>>');"><<:os:>></td>
    <td align=right class="table_row"><a class="link" href="javascript:void(0)" onClick="return setSelectOption(document.forms['form_build'], 'filter_arch', '<<:arch js:>>');"><<:arch:>></td>
    <td align=right class="table_row"><a class="link" href="javascript:void(0)" onClick="return setSelectOption(document.forms['form_build'], 'filter_build_version', '<<:build_version js:>>');"><<:build_version:>></td>
    <td align=right class="table_row"><font color="<<:make-patch_result:>>"><<:make-patch_time:>></font></td>
    <td align=right class="table_row"><font color="<<:make_result:>>"><<:make_time:>></font></td>
    <td align=right class="table_row"><font color="<<:make-install_result:>>"><<:make-install_time:>></font></td>
    <td align=right class="table_row"><font color="<<:make-test_result:>>"><<:make-test_time:>></font></td>
    <td align=right class="table_row"><font color="<<:make-cache_result:>>"><<:make-cache_time:>></font></td>
    <td align=right class="table_row"><font color="<<:make-autopackage_result:>>"><<:make-autopackage_time:>></font></td>
</tr>
